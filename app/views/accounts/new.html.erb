<% provide(:title, 'New Account') %>
<h1>New Account</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@account) do |f| %>

      <br><br>
      <%= f.submit "Save", class: "btn btn-primary" %>
    <% end %>
    <br>
    <%= link_to "Cancel", :back, class: "btn btn-default" %>
    <br><br>
  </div>
</div>